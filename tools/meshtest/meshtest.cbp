				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-DNDEBUG" />
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="OgreMain" />
					<Add library="OIS" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-fident" />
			<Add option="-DWIN32" />
			<Add option="-D_WINDOWS" />
