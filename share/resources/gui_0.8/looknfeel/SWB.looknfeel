                <Image component="TopRightCorner" name="SWB/Rounded_Square_Small_TR" />
            </FrameComponent>
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" >
                        <OperatorDim op="Add">
