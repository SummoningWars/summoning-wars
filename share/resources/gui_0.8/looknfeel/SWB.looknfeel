                <Area>
                    <!--
                    <Dim type="LeftEdge"><ImageDim name="SWB/PanelTitleArea_Calc" dimension="LeftEdge" /></Dim>
                    <Dim type="TopEdge"><ImageDim name="SWB/PanelTitleArea_Calc" dimension="TopEdge" /></Dim>
                    -->
