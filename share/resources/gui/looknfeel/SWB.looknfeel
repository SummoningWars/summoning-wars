                <Image type="BottomLeftCorner" imageset="SWB" image="ListboxBottomLeft" />
                <Image type="BottomRightCorner" imageset="SWB" image="ListboxBottomRight" />
                <Image type="LeftEdge" imageset="SWB" image="ListboxLeft" />
                <Image type="RightEdge" imageset="SWB" image="ListboxRight" />
                <Image type="TopEdge" imageset="SWB" image="ListboxTop" />
                <Image type="BottomEdge" imageset="SWB" image="ListboxBottom" />
                <Image type="Background" imageset="SWB" image="ListboxBackdrop" />
